        android:layout_gravity="center"
        android:layout_marginTop="250dp"
        android:id="@+id/logOutMainActivity"
        android:background="@color/black"
        android:textColor="@color/white"
        android:textSize="30sp"
        android:textStyle="bold"/>



</LinearLayout>